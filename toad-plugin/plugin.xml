<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="importerExporterPropertyExtension" name="Importer/Exporter property" schema="schema/importerExporterPropertyExtension.exsd"/>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <newWizardShortcut
               id="jp.hishidama.toadEditor.wizard.new-batch">
         </newWizardShortcut>
         <newWizardShortcut
               id="jp.hishidama.toadEditor.wizard.new-jobflow">
         </newWizardShortcut>
         <newWizardShortcut
               id="jp.hishidama.toadEditor.wizard.new-flowpart">
         </newWizardShortcut>
         <newWizardShortcut
               id="jp.hishidama.toadEditor.wizard.new-importDiagram">
         </newWizardShortcut>
      </perspectiveExtension>
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaBrowsingPerspective">
         <newWizardShortcut
               id="jp.hishidama.toadEditor.wizard.new-batch">
         </newWizardShortcut>
         <newWizardShortcut
               id="jp.hishidama.toadEditor.wizard.new-jobflow">
         </newWizardShortcut>
         <newWizardShortcut
               id="jp.hishidama.toadEditor.wizard.new-flowpart">
         </newWizardShortcut>
         <newWizardShortcut
               id="jp.hishidama.toadEditor.wizard.new-importDiagram">
         </newWizardShortcut>
      </perspectiveExtension>
   </extension>
   <extension
         id="jp.hishidama.toadEditor.newWizard"
         name="Asakusa Toad Editor New Wizard"
         point="org.eclipse.ui.newWizards">
      <category
            id="jp.hishidama.toadEditor.category.new-wizard"
            name="Asakusa Toad Editor">
      </category>
      <wizard
            category="jp.hishidama.toadEditor.category.new-wizard"
            class="jp.hishidama.eclipse_plugin.toad.wizard.newdiagram.NewBatchWizard"
            icon="icons/toad.gif"
            id="jp.hishidama.toadEditor.wizard.new-batch"
            name="バッチダイアグラム">
         <description>
            バッチダイアグラム（Batch DSLのtoadファイル）を作成します。
         </description>
      </wizard>
      <wizard
            category="jp.hishidama.toadEditor.category.new-wizard"
            class="jp.hishidama.eclipse_plugin.toad.wizard.newdiagram.NewJobflowWizard"
            icon="icons/toad.gif"
            id="jp.hishidama.toadEditor.wizard.new-jobflow"
            name="ジョブフローダイアグラム">
         <description>
            ジョブフローダイアグラム（Flow DSL（JobFlow）のtoadファイル）を作成します。
         </description>
      </wizard>
      <wizard
            category="jp.hishidama.toadEditor.category.new-wizard"
            class="jp.hishidama.eclipse_plugin.toad.wizard.newdiagram.NewFlowpartWizard"
            icon="icons/toad.gif"
            id="jp.hishidama.toadEditor.wizard.new-flowpart"
            name="フロー演算子ダイアグラム">
         <description>
            フロー演算子ダイアグラム（Flow DSL（FlowPart）のtoadファイル）を作成します。
         </description>
      </wizard>
      <wizard
            category="jp.hishidama.toadEditor.category.new-wizard"
            class="jp.hishidama.eclipse_plugin.toad.wizard.newdiagram.ReverseWizard"
            icon="icons/toad.gif"
            id="jp.hishidama.toadEditor.wizard.new-importDiagram"
            name="既存DSLクラスからダイアグラム生成">
         <description>
            既存のDSL（Javaクラス）からダイアグラム（toadファイル）を生成します。
         </description>
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="jp.hishidama.eclipse_plugin.toad.editor.ToadEditor"
            contributorClass="jp.hishidama.eclipse_plugin.toad.editor.menu.ToadActionBarContributor"
            default="false"
            filenames="*.btoad"
            icon="icons/toad.gif"
            id="jp.hishidama.toadEditor"
            name="Asakusa Toad Editor">
      </editor>
      <editor
            class="jp.hishidama.eclipse_plugin.toad.editor.ToadEditor"
            contributorClass="jp.hishidama.eclipse_plugin.toad.editor.menu.ToadActionBarContributor"
            default="false"
            filenames="*.jtoad"
            icon="icons/toad.gif"
            id="jp.hishidama.toadEditor"
            name="Asakusa Toad Editor">
      </editor>
      <editor
            class="jp.hishidama.eclipse_plugin.toad.editor.ToadEditor"
            contributorClass="jp.hishidama.eclipse_plugin.toad.editor.menu.ToadActionBarContributor"
            default="false"
            filenames="*.ftoad"
            icon="icons/toad.gif"
            id="jp.hishidama.toadEditor"
            name="Asakusa Toad Editor">
      </editor>
   </extension>
