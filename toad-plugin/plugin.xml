   <extension
         point="org.eclipse.ui.commands">
      <category
            id="jp.hishidama.toadEditor.command.category"
            name="Asakusa Toad Editor">
      </category>
      <command
            categoryId="jp.hishidama.toadEditor.command.category"
            id="jp.hishidama.toadEditor.command.generateDslClass"
            name="Generate DSL Class">
      </command>
      <command
            categoryId="jp.hishidama.toadEditor.command.category"
            id="jp.hishidama.toadEditor.command.generateBatchDiagram"
            name="Generate Toad Diagram">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="jp.hishidama.eclipse_plugin.toad.editor.handler.GenerateDslClassHandler"
            commandId="jp.hishidama.toadEditor.command.generateDslClass">
      </handler>
      <handler
            class="jp.hishidama.eclipse_plugin.toad.editor.handler.ReverseHandler"
            commandId="jp.hishidama.toadEditor.command.generateBatchDiagram">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="popup:org.eclipse.jdt.ui.PackageExplorer?after=additions">
         <menu
               label="Asakusa Toad Editor">
            <command
                  commandId="jp.hishidama.toadEditor.command.generateDslClass"
                  style="push">
               <visibleWhen>
                  <reference
                        definitionId="jp.hishidama.toadEditor.definition.toad">
                  </reference>
               </visibleWhen>
            </command>
            <command
                  commandId="jp.hishidama.toadEditor.command.generateBatchDiagram"
                  style="push">
               <visibleWhen>
                  <reference
                        definitionId="jp.hishidama.toadEditor.definition.class">
                  </reference>
               </visibleWhen>
            </command>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.core.expressions.definitions">
      <definition
            id="jp.hishidama.toadEditor.definition.toad">
         <with
               variable="selection">
            <iterate
                  ifEmpty="false">
               <or>
               <adapt
                     type="org.eclipse.core.resources.IFolder">
               </adapt>
               <adapt
                     type="org.eclipse.core.resources.IFile">
                  <or>
                  <test
                        property="org.eclipse.core.resources.extension"
                        value="btoad">
                  </test>
                  <test
                        property="org.eclipse.core.resources.extension"
                        value="jtoad">
                  </test>
                  <test
                        property="org.eclipse.core.resources.extension"
                        value="ftoad">
                  </test>
                  </or>
               </adapt>
               </or>
            </iterate>
         </with>
      </definition>
      <definition
            id="jp.hishidama.toadEditor.definition.class">
         <with
               variable="selection">
            <iterate
                  ifEmpty="false">
               <adapt
                     type="org.eclipse.jdt.core.ICompilationUnit">
               </adapt>
            </iterate>
         </with>
      </definition>
   </extension>
   <extension
         point="org.eclipse.ui.workbench.texteditor.hyperlinkDetectors">
      <hyperlinkDetector
            activate="true"
            class="jp.hishidama.eclipse_plugin.toad.jdt.hyperlink.OpenToadHyperlinkDetector"
            id="jp.hishidama.toadEditor.hyperlinkDetector.jdt.open-toad"
            name="Open Asakusa Toad diagram"
            targetId="org.eclipse.jdt.ui.javaCode">
      </hyperlinkDetector>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="jp.hishidama.toadEditor.views.category"
            name="Asakusa Toad Editor">
      </category>
      <view
            category="jp.hishidama.toadEditor.views.category"
            class="jp.hishidama.eclipse_plugin.toad.view.SiblingDataModelView"
            id="jp.hishidama.toadEditor.view.siblingDataModelView"
            name="Sibling DataModel"
            restorable="true">
      </view>
   </extension>
   <extension
         point="jp.hishidama.toadEditor.importerExporterPropertyExtension">
